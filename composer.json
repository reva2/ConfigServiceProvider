{
    "name": "reva2/config-service-provider",
    "description": "A config ServiceProvider for Silex 2.x with support for php, json and yaml.",
    "keywords": ["silex"],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/reva2/ConfigServiceProvider/graphs/contributors"
        }
    ],
    "require": {
        "silex/silex": "~2.0"
    },
    "require-dev": {
        "symfony/yaml": "~3.1",
        "jamesmoss/toml": "~1.1",
        "phpunit/phpunit": "~4.8"
    },
    "suggest": {
        "symfony/yaml": "~3.1",
        "jamesmoss/toml": "~1.1"
    },
    "autoload": {
        "psr-0": { "Igorw\\Silex": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
